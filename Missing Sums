https://www.codechef.com/COOK135C/problems/MISSSUMS

#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n,i=1;
	    cin>>n;
	    vector<int>v;
	    while(v.size()!=n)
	    {
	        if(i%2)
	        v.push_back(i);
	        i++;
	    }
	    for(auto x:v)
	    cout<<x<<" ";
	    cout<<"\n";
	}
	return 0;
}
